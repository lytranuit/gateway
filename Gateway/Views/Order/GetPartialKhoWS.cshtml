
@model APIInvoice.Data
<table id="example" class="table-bordered display nowrap" style="width:100%">
    <thead>
        <tr>
            <th class="center"></th>
            <th class="center">STT</th>
            <th class="text-center">Số HDNB</th>
            <th class="text-center">Số HDDT</th>
            <th class="text-center">Ngày lập HDDT</th>
            <th class="text-center">Ngày phát hành</th>
            <th class="text-center">Mã KH</th>
            <th class="text-center">Đơn vị</th>
            <th class="text-center">Ký hiệu</th>

        </tr>
    </thead>
    <tfoot>
        <tr>
            <th class="center"></th>
            <th class="center">STT</th>
            <th class="text-center">Số HDNB</th>
            <th class="text-center">Số HDDT</th>
            <th class="text-center">Ngày lập HDDT</th>
            <th class="text-center">Ngày phát hành</th>
            <th class="text-center">Mã KH</th>
            <th class="text-center">Đơn vị</th>
            <th class="text-center">Ký hiệu</th>
        </tr>
    </tfoot>
    <tbody id="Partialtable">
        @{
            var count = 1;
            foreach (var i in Model.Item)
            {
                <tr>
                    <td class="p-1 align-middle text-center">
                                <div class="btn-group">
                                    <a target="_blank" data-sohdnb="@i.Fkey" data-sohddt="@Int32.Parse(i.InvoiceNo).ToString("00000000")" data-kyhieu="@i.Serial" href="/@ViewBag.flag/crm/in-phieu-xuat-kho-ws?sohdnb=@i.Fkey&kyhieu=@i.Serial" class="btnchitiet btn btn-primary rounded-0 pl-2 pr-2"><span class="fa fa-print"></span></a>
                                    @*<button type="button" class="btn btn-primary rounded-0 pl-2 pr-2" data-toggle="dropdown">
                                        
                                    </button>*@
                                    @*<ul class="dropdown-menu text-right">

            <li></li>

        </ul>*@
                                </div>
                    </td>
                    <td class="text-center">@count</td>

                    <td>@i.Fkey</td>
                    <td class="text-center">@Int32.Parse(i.InvoiceNo).ToString("00000000")</td>
                    <td>@i.ArisingDate</td>
                    <td>@i.PublishDate</td>
                    <td>@i.CusCode</td>
                    <td>@i.CusName</td>
                    <td>@i.Serial</td>

                </tr>
                count = count + 1;
            }
        }
    </tbody>
</table>