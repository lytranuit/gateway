                                type: "POST",
                                url: "@Url.Action("EditThongtin", "Home")",
                                contentType: "application/json",
                                datatype: 'json',
                                data: '{email: ' + JSON.stringify(input.val()) + '}',
                                error: function () {
                                    toastr.options = {
                                        "closeButton": true,
                                        "debug": false,
                                        "newestOnTop": true,
                                        "progressBar": true,
                                        "positionClass": "toast-top-right",
                                        "preventDuplicates": false,
                                        "onclick": null,
                                        "showDuration": "300",
                                        "hideDuration": "1000",
                                        "timeOut": "5000",
                                        "extendedTimeOut": "1000",
                                        "showEasing": "swing",
                                        "hideEasing": "linear",
                                        "showMethod": "fadeIn",
                                        "hideMethod": "fadeOut"
                                    }
                                    Command: toastr["warning"]("Vui lòng thử lại", "Lỗi")
                                },
                                success: function (data) {
                                    toastr.options = {
                                        "closeButton": true,
                                        "debug": false,
                                        "newestOnTop": true,
                                        "progressBar": true,
                                        "positionClass": "toast-top-right",
                                        "preventDuplicates": false,
                                        "onclick": null,
                                        "showDuration": "300",
                                        "hideDuration": "1000",
                                        "timeOut": "5000",
                                        "extendedTimeOut": "1000",
                                        "showEasing": "swing",
                                        "hideEasing": "linear",
                                        "showMethod": "fadeIn",
                                        "hideMethod": "fadeOut"
                                    }
                                    Command: toastr["success"]("Bạn đã cập nhật thành công địa chỉ Email, khi quên mật khẩu hãy nhập email này để hệ thống cấp lại mật khẩu !", "THÔNG BÁO")
                                },
                                timeout: 5000,
                            });
                        }
                    }
                },
                //        cancel:
                //{
                //    text: 'Để sau',
                //    btnClass: 'btn-danger',
                //    action: function () {
                //    }

                //}
            }
                });
            });
        </script>

    }
