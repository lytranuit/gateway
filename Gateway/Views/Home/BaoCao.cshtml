                        {
                            <button id="pdf" type="submit" name="btnin" value="2" class="w3-bar-item w3-button"><img style="margin-bottom:0;" src="~/Content/Image/pdf4.png">&nbsp; &nbsp; &nbsp; PDF</button>
                        }
