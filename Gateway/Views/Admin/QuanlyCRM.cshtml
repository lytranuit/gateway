
@{
    ViewBag.Title = "QuanlyCRM";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<link rel="stylesheet" href="~/Content/CheckBox.css">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
@*<link href="~/Content/bootstrap-select.css" rel="stylesheet">*@
<link href="~/Content/Material.css" rel="stylesheet">
<script src="~/Scripts/bootbox.min.js"></script>

<div class="container-fluid">
    <!-- Start Page Content -->
    <div class="row">
        <div class="col-12">
            <div class="card boxshadow">
                <div class="card-body">
                  
                    <div class="table-responsive">
                        <table id="example23" class="display nowrap table table-hover table-bordered" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>STT</th>
                                    <th>Tài khoản</th>
                                    <th>Mã CN</th>
                                    <th>Quyền</th>
                                    <th class="text-left">Mã TDV</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    int count = 1;
                                    foreach (var i in ViewBag.data)
                                    {
                                        <tr>
                                            <th>@count</th>
                                            <td>@i.taikhoan</td>
                                            <td>@i.macn</td>
                                            <td>@i.quyen</td>
                                            <td class="text-left">@i.matdv</td>
                                        </tr>
                                        count = count + 1;
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <!-- End PAge Content -->
</div>
<script src="~/Content/AdminLayout/js/lib/datatables/datatables.min.js?version=3"></script>
